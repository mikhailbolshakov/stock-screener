{
  "version": "1.0",
  "local_store": "/var/lib/git2consul_cache",
  "repos": [
    {
      "name": "config",
      "expand_keys" : true,
      "ignore_file_extension" : true,
      "url": "git@github.com:mikhailbolshakov/stock-screener.git",
      "source_root": "config",
      "mountpoint": "stock-screener",
      "branches": [
        "master"
      ],
      "include_branch_name": false,
      "hooks": [
        {
          "type": "polling",
          "interval": "1"
        }
      ]
    }
  ]
}